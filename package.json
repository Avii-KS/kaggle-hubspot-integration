{
  "name": "kaggle-hubspot-integration",
  "version": "1.0.0",
  "description": "Automated data pipeline to scrape baby names from Kaggle and sync to HubSpot CRM",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "ts-node src/index.ts",
    "migrate": "npm run build && npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migration:generate": "npx sequelize-cli migration:generate --name"
  },
  "keywords": [
    "kaggle",
    "hubspot",
    "playwright",
    "sequelize",
    "typescript",
    "data-pipeline"
  ],
  "author": "Avinash Kumar",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.8.1",
    "@types/sequelize": "^4.28.20",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hubspot/api-client": "^13.4.0",
    "adm-zip": "^0.5.16",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "mysql2": "^3.15.2",
    "playwright": "^1.56.1",
    "sequelize": "^6.37.7",
    "winston": "^3.18.3"
  }
}
